{
  "compilerOptions": {
    "target": "ES2018",
    "module": "ESNext",
    "moduleResolution": "Node",
    "lib": [
      "ESNext",
      "ESNext.AsyncIterable",
      "DOM"
    ],
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "experimentalDecorators": true,
    "baseUrl": "source",
    "resolveJsonModule": true,
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ]
    },
    "types": [
      "vue/types",
      "@nuxt/types",
      "@types/node",
      "@types/w3c-css-typed-object-model-level-1"
    ]
  },
  "exclude": [
    ".nuxt",
    ".firebase",
    "funtions",
    "node_modules",
    "app",
  ]
}
